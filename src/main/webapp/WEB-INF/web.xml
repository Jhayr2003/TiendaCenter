<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>SvHome</servlet-name>
        <servlet-class>servlets.SvHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvVerificarCuenta</servlet-name>
        <servlet-class>servlets.SvVerificarCuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hola</servlet-name>
        <servlet-class>servlets.hola</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvRegistrarCuenta</servlet-name>
        <servlet-class>servlets.SvRegistrarCuenta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SvHome</servlet-name>
        <url-pattern>/SvHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvVerificarCuenta</servlet-name>
        <url-pattern>/SvVerificarCuenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hola</servlet-name>
        <url-pattern>/hola</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvRegistrarCuenta</servlet-name>
        <url-pattern>/SvRegistrarCuenta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
